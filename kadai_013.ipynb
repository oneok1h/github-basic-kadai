      "source": [
        "def product(price,consumption_tax):\n",
        "    # 引数price(商品の金額)と引数consumption_tax(消費税)を掛けてpriceに加算し、変数totalに代入する\n",
        "    total = price * consumption_tax + price\n",
        "\n",
        "    # 変数totalの値を出力する\n",
        "    print(f\"{total}円\")\n",
        "\n",
        " # 関数を呼び出し、引数として商品の金額と送料を渡す\n",
        "product(1000,0.1)\n"
